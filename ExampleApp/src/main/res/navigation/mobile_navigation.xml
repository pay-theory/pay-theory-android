<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.paytheory.android.example.ui.credit.OldCardFragment"
        android:label="@string/credit_card"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.paytheory.android.example.ui.ach.AchFragment"
        android:label="@string/ach"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.paytheory.android.example.ui.cash.CashFragment"
        android:label="@string/cash"
        tools:layout="@layout/fragment_notifications" />
</navigation>